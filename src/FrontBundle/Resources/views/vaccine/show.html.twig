{% extends '@Front/base.html.twig' %}

{% block title %}
{{ parent() }} - vaccine
{% endblock %}

{% block body %}
    <br><br><br><br><br><br><br><br>


<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div class="page-header">
            <h2>View vaccine <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
        </div>
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ vaccine.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Name</strong></p>
        <p>
            {{ vaccine.name }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Date</strong></p>
        <p>
            {% if vaccine.date %}{{ vaccine.date|date('Y-m-d') }}{% endif %}
        </p>

    </div>
     </div>
    </div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('vaccine') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list</a>
</div>


{% endblock %}

